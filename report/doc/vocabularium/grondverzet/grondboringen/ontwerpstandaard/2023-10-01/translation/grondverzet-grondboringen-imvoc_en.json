    {
      "EA-Guid": "{BA85C1A3-5058-41f8-9C84-FDB92B01C7BC}",
      "name": "Boormethodeinterval",
      "label": {
        "nl": "BoormethodeInterval",
        "en": "Enter your translation here"
      },
      "definition": {
        "nl": "Beschrijving van de Boormethode voor een bepaald diepte-interval.",
        "en": "Enter your translation here"
      },
      "usage": {
        "nl": "Specialisatie van een CurveWaardepaar, dit is een Curve en de waarde die eraan is toegekend. De waarde is hier de beschrijving van de Boormethode van een Grondboring, terwijl de Curve aangeeft op welke dieptes deze beschrijving geldig is. De Curve van de Grondboring wordt hierbij als referentiesysteem gebruikt. OPGELET: Als de Curve van de Grondboring niet recht in de grond werd geboord zijn de opgegeven dieptes eerder afstanden langs deze Curve.",
        "en": "Enter your translation here"
      }
    },
    {
      "EA-Guid": "{82CE4FE2-7C8F-4038-92DF-6B1E0BBBA73E}",
      "name": "Grondboring",
      "label": {
        "nl": "Grondboring",
        "en": "Enter your translation here"
      },
      "definition": {
        "nl": "De handeling van het boren van een smalle schacht in de grond.",
        "en": "Enter your translation here"
      },
      "usage": {
        "nl": "Onder \"grond\" verstaan we hier het deel van de aardkorst onder het aardoppervlak. Het slaat zowel op de bodem als op wat zich daaronder bevindt (en hier ondergrond wordt genoemd).",
        "en": "Enter your translation here"
      }
    }
