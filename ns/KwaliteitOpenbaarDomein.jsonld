    {
      "@id": "https://implementatie.data.test-vlaanderen.be/ns/kwaliteit-wegen-en-wegmarkeringen#Conformiteitsresultaat",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Conformiteitsresultaat"
      },
      "definition": {
        "nl": "Informatie over het resultaat van de evaluatie van de verkregen waarde(n) volgens een gespecifieerd conformiteitskwaliteitsniveau."
      },
      "usage": {
        "nl": "http://def.isotc211.org/iso19157/2013/DataQualityResult#DQ_ConformanceResult"
      },
      "parents": [
        "https://implementatie.data.test-vlaanderen.be/ns/kwaliteit-wegen-en-wegmarkeringen#KwaliteitsResultaat"
      ]
    },
    {
